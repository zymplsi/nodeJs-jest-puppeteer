{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "linterOptions": {
        "exclude": [
            "node_modules/**"
        ]
    },
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single", "jsx-double"],
        "arrow-parens":false,
        "trailing-comma": false,
        "no-console": false,
        "object-literal-sort-keys" : false
    },
    "rulesDirectory": []
}